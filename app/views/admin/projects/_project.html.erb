<tr id="project-item-<%= project.id %>">
  <td><%= project.title %></td>
  <td><%= project.client_company %></td>
  <td>
    <%= link_to 'Show', admin_project_path(project), class: 'btn btn-info', data: { controller: "bs-modal-form" } %>
    <%= link_to 'Edit', edit_admin_project_path(project), class: 'btn btn-warning', data: { controller: "bs-modal-form" } %>
    <%= link_to 'Delete', admin_project_path(project), class: 'btn btn-danger', data: { turbo_method: :delete, turbo_confirm: 'Are you sure you want to delete this project? This action can not be undo.' } %>
  </td>
</tr>

<%= link_to "New Project", new_admin_project_path, class: "btn btn-primary mb-3", data: { controller: "bs-modal-form" } %>

<table class="table table-bordered">
  <thead>
    <tr>
      <th>Title</th>
      <th>Company</th>
      <th colspan="3">Actions</th>
    </tr>
  </thead>
  <tbody id="project-list">
    <% @projects.each do |project| %>
      <%= render "admin/projects/project", project: project %>
    <% end %>
  </tbody>
</table>

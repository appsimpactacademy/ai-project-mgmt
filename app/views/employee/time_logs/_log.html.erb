<tr id="time-log-item-<%= time_log.id %>">
  <td><%= time_log.log_date %></td>
  <td><%= time_log.employee_project.project.title %></td>
  <td><%= "#{time_log.time_in_hours} hours" %></td>
  <td>
    <%= link_to 'Show', employee_time_log_path(time_log), class: 'btn btn-info', data: { controller: "bs-modal-form" } %>
    <%= link_to 'Edit', edit_employee_time_log_path(time_log), class: 'btn btn-warning', data: { controller: "bs-modal-form" } %>
    <%= link_to 'Delete', employee_time_log_path(time_log), class: 'btn btn-danger', data: { turbo_method: :delete, turbo_confirm: 'Are you sure you want to delete this time_log? This action can not be undo.' } %>
  </td>
</tr>
